{
  "name": "@hawtio/synapse-terminal",
  "version": "0.0.1",
  "description": "An AI Rest Agent for communicating with LLMs about Hawtio",
  "private": true,
  "author": "Hawtio developer team",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hawtio/hawtio-synapse.git"
  },
  "keywords": [
    "hawtio",
    "jolokia",
    "OpenShift",
    "AI"
  ],
  "scripts": {
    "start": "yarn cert-gen && yarn server",
    "server": "tsup --config tsup.config.dev.ts --watch",
    "cert-gen": "cd tls-certificates && ./generate-dev-certs.sh",
    "build": "webpack --mode production --progress --config webpack.config.js",
    "test": "yarn create-test-certs && jest --detectOpenHandles",
    "test:watch": "yarn create-test-certs && jest --detectOpenHandles --watch",
    "test:coverage": "yarn create-test-certs && jest --coverage",
    "create-test-certs": "cd test-tls && ./create-cert.sh",
    "lint": "yarn eslint .",
    "lint:fix": "yarn eslint . --fix",
    "format:check": "yarn prettier --check .",
    "format:fix": "yarn prettier --write ."
  },
  "dependencies": {
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@langchain/google-genai": "^0.2.13",
    "@langchain/mistralai": "^0.2.1",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.5.15",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "fetch-intercept": "^2.4.0",
    "helmet": "^6.0.0",
    "https": "^1.0.0",
    "jwt-decode": "^4.0.0",
    "langchain": "^0.3.29",
    "method-override": "^3.0.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.2",
    "yaml": "^2.4.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.28.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jquery": "^3.5.31",
    "@types/js-yaml": "^4.0.9",
    "@types/method-override": "^0.0.35",
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.12",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.6.1",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "jest": "^29.6.1",
    "jest-extended": "^4.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "3.5.3",
    "source-map-loader": "^5.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.8.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@4.3.1",
  "engines": {
    "yarn": ">=4.3.1",
    "node": ">=20"
  },
  "resolutions": {
    "@types/express-serve-static-core": "^4.17.21",
    "@types/express": "^4.17.21"
  }
}
